{
    "manifest_version": 3,
    "name": "WhatsApp New Chat",
    "version": "1.0",
    "description": "Start a new chat in WhatsApp",
    "permissions": [
        "tabs", "scripting"
    ],
    "host_permissions": [
        "https://web.whatsapp.com/*"
    ],
    "action": {
        "default_icon": {
            "128": "icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["https://web.whatsapp.com/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "128": "icon128.png"
    }
}
