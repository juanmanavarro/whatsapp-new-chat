{
    "manifest_version": 3,
    "name": "WhatsApp New Chat",
    "version": "1.3",
    "description": "Start a new chat in WhatsApp",
    "action": {
        "default_icon": {
            "128": "icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["https://web.whatsapp.com/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "128": "icon128.png"
    },
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["avatar.webp"]
    }]
}
